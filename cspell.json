{
  "caseSensitive": false,
  "dictionaries": [
    "en_US",
    "softwareTerms",
    "svelte",
    "typescript",
    "npm",
    "node",
    "html",
    "html-symbol-entities",
    "css",
    "fonts",
    "bash",
    "filetypes",
    "misc",
    "creators",
    "compound-words",
