    {
      "name": "technical-terms",
      "path": "./.cspell/technical-terms.txt",
      "addWords": true,
      "supportNonStrictSearches": true
    }
  ],
  "ignoreRegExpList": [
    "/\\b[a-zA-Z0-9]{16}\\b/g",
